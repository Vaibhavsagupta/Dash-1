# PostgreSQL Database (Required)
# For local development, you can use a local PostgreSQL instance or a cloud service like Supabase
DATABASE_URL=postgresql://user:password@host:5432/database_name

# Example for Supabase:
# DATABASE_URL=postgresql://postgres:your_password@db.xxxxx.supabase.co:5432/postgres

# Example for local PostgreSQL:
# DATABASE_URL=postgresql://postgres:password@localhost:5432/dashboard_db

# Auth Configuration
SECRET_KEY=your_super_secret_key_change_this_to_something_random
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Frontend URL (Update this when deploying)
FRONTEND_URL=http://localhost:5173
